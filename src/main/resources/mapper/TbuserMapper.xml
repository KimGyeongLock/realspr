<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0/EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.thc.realspr.mapper.TbuserMapper">

	<select id="detail" parameterType="hashMap" resultType="com.thc.realspr.dto.TbuserDto$TbuserSelectDto">
		SELECT tbuser.id
			,tbuser.deleted
			,tbuser.created_at as createdAt
			,tbuser.modified_at as modifiedAt
			,tbuser.username
			,tbuser.name
			,tbuser.nick
			,tbuser.phone
			,tbuser.mpic
			,tbuser.content
		FROM tbuser
		WHERE tbuser.id = #{id}
	</select>
	<select id="list" parameterType="hashMap" resultType="com.thc.realspr.dto.TbuserDto$TbuserSelectDto">
		SELECT tbuser.id
		FROM tbuser
		WHERE tbuser.id is not null
	</select>
